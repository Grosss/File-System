@using PL.Models
@model IEnumerable<FileViewModel>

@foreach (FileViewModel file in Model)
{
    <tr class="file">
        <td class="fileName">
            @file.Name
        </td>
        <td>@file.LastAccessTime</td>
        <td>@file.Extension.ToUpper().Replace(".", "") File</td>
        <td>@file.FileSizeText</td>
    </tr>
}